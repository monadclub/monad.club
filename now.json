{
	"name": "monad.club",
	"version": 2,
	"alias": "monad.club",
	"public": false,
	"regions": [ "all" ],
	"env": {},
	"builds": [
		{ "src": "./packages/www/static/*", "use": "@now/static" },
		{ "src": "./packages/www/next.config.js", "use": "@now/next" },
		{ "src": "./packages/api/lib/api.ts", "use": "@now/node-server" }
	],
	"routes": [
		{ "src": "^/graphql", "dest": "/packages/api/lib/api.ts" },
		{ "src": "^/static/(.*)", "dest": "/packages/www/static/$1" },
		{ "src": "/(.*)", "dest": "/packages/www/$1" }
	]
}
